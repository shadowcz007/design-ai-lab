  "code": "var audio\nwindow.volume = 0\nwindow.margin = 0\nwindow.amplitude = window.innerHeight * 4\nwindow.scale = 10\n\nvar ctx, interval, ctxForRecord\n\nlet playIcon,mediaRecorder;\n\nfunction gui () {\n  playIcon = Lab.ui.createIcon('play', mousePressed)\n  Lab.ui.add(playIcon)\n  playIcon.style = `position: fixed;\n  top: 12px;\n  left: 12px;`\n\n  let canvas = Lab.ui.createCanvas(\n    window.innerWidth,\n    window.innerHeight,\n    '',\n    '',\n    true\n  )\n  canvas.style.outline = '1px solid gray'\n  ctx = canvas.getContext('2d')\n  Lab.ui.add(canvas)\n  let canvasForRecord = Lab.ui.createCanvas(\n    window.innerWidth,\n    window.innerHeight,\n    '',\n    '',\n    true\n  )\n  ctxForRecord = canvasForRecord.getContext('2d')\n}\n\nfunction mousePressed (e) {\n  if (!audio) {\n    window.audioUrl = Lab.ui.getFilePath(1)[0]\n    if (window.audioUrl) {\n      audio = new Lab.Audio()\n      audio.init(window.audioUrl).then(() => {\n        draw()\n        playIcon.querySelector('i').className = 'icon pause'\n        exportVideo(audio.audio.duration * 1000)\n      })\n    }\n  } else {\n    // Clean up our element and audio context\n    stop()\n  }\n}\n\nfunction stop () {\n  audio.stop()\n  audio = null;\n  if(mediaRecorder)mediaRecorder.stop();\n  playIcon.querySelector('i').className = 'icon play';\n}\n\nfunction draw () {\n  if(!audio)return;\n  const size = window.amplitude / 2\n  let { points, mean: radius, currentTime, duration } = audio.output(\n    0,\n    window.innerWidth,\n    window.innerHeight / 2 - size,\n    window.innerHeight / 2 + size\n  )\n\n  //   上一个点位\n  let prePoint\n\n  ctx.fillStyle = `black`\n\n  ctx.clearRect(0, 0, window.innerWidth, window.innerHeight)\n  ctx.stroke()\n  \n  // ctx.stroke()\n  ctx.beginPath()\n\n  ctx.moveTo(window.innerWidth / 2+window.scale * radius,window.innerHeight / 2)\n  // 画圆\n  ctx.arc(\n    window.innerWidth / 2,\n    window.innerHeight / 2,\n    window.scale * radius,\n    0,\n    2 * Math.PI\n  )\n\n  for (let i = 0; i < points.length; i++) {\n    if (prePoint) {\n      ctx.moveTo(...prePoint)\n      ctx.lineTo(points[i].x, points[i].y)\n    }\n    prePoint = [points[i].x, points[i].y]\n  }\n\n  ctx.closePath()\n  ctx.stroke()\n\n  if (duration > 0) {\n    let cropCanvas = Lab.ui.createCanvas(window.innerWidth, window.innerHeight)\n    let cropCtx = cropCanvas.getContext('2d')\n    cropCtx.drawImage(ctx.canvas, 0, 0, window.innerWidth, window.innerHeight)\n    cropCtx.globalCompositeOperation = 'source-out'\n    cropCtx.fillRect(\n      0,\n      0,\n      (window.innerWidth * currentTime) / duration,\n      window.innerHeight\n    )\n    ctx.fillStyle = 'white'\n    ctx.fillRect(\n      0,\n      0,\n      (window.innerWidth * currentTime) / duration,\n      window.innerHeight\n    )\n    ctx.drawImage(cropCanvas, 0, 0, window.innerWidth, window.innerHeight)\n  }\n\n  // 用来录制\n  ctxForRecord.fillStyle = 'white'\n  ctxForRecord.fillRect(0, 0, window.innerWidth, window.innerHeight)\n  ctxForRecord.drawImage(\n    ctx.canvas,\n    0,\n    0,\n    window.innerWidth,\n    window.innerHeight\n  )\n\n  requestAnimationFrame(draw)\n}\n\nasync function exportVideo (time = 1000) {\n  // var chunks = []\n  let astream = audio.audio.captureStream()\n  let vstream = ctxForRecord.canvas.captureStream(24)\n  vstream.addTrack(astream.getAudioTracks()[0])\n  mediaRecorder = new MediaRecorder(vstream);\n  mediaRecorder.chunks=[];\n  mediaRecorder.start()\n  mediaRecorder.ondataavailable = function (e) {\n   if(mediaRecorder) mediaRecorder.chunks.push(e.data)\n  }\n  mediaRecorder.onstop = async e => {\n    var blob = new Blob(mediaRecorder.chunks, { type: 'video/webm' })\n    mediaRecorder.chunks=[];\n    mediaRecorder=null;\n    var url = URL.createObjectURL(blob)\n    let v = await Lab.ui.createVideo(url)\n    Lab.ui.add(v)\n    stop()\n  }\n  Lab.base.sleep(time).then(() => {\n   if(mediaRecorder) mediaRecorder.stop()\n  })\n}\n",
