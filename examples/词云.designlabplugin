  "code": "// 主题色\nlet _COLORS;\nlet db = new Lab.Store('wordcloud');\n\nfunction gui() {\n    Lab.ui.p5Show(false);\n    let mainDiv, canvas, pasteBtn, copyBtn, runBtn, input;\n\n    mainDiv = Lab.ui.createGroup('container');\n    mainDiv.style = `\n    width:100%;\n    height:100vh;\n    padding:44px\n    `;\n\n    canvas = Lab.ui.createBaseCanvas(300, 300);\n    canvas.style = `\n    margin:12px;\n    outline:1px solid gray`;\n\n\n    pasteBtn = Lab.ui.createButton('粘贴文本', () => {\n        let text = Lab.clipboard.read('text');\n        let texts = Lab.nlp.jieba.extract(text, 20);\n        texts = Array.from(texts, t => t.keyword + \" \" + t.weight * 100).join('\\n');\n        input.setValue(texts);\n        createCard(canvas, texts);\n    });\n    pasteBtn.style = `margin:12px;`;\n\n    copyBtn = Lab.ui.createButton('拷贝至剪切板', () => {\n        Lab.clipboard.write(canvas.toDataURL('png'), 'base64')\n    });\n    copyBtn.style = `margin:12px;`;\n\n    input = Lab.ui.createTextareaInput('', e => {\n        // console.log(canvas.width)\n        createCard(canvas, e.target.value);\n    });\n\n    runBtn = Lab.ui.createButton('生成', () => {\n        let t = input.getValue();\n        createCard(canvas, t);\n    });\n    runBtn.style = `margin:12px;`;\n\n    Lab.ui.add(mainDiv);\n    mainDiv.add(input);\n    mainDiv.add(pasteBtn);\n    mainDiv.add(runBtn);\n    mainDiv.add(canvas);\n    mainDiv.add(copyBtn);\n\n    // 从缓存读取\n    db.get('data').then(r => {\n        input.setValue(r || '');\n    });\n    db.get('colors').then(cs => {\n        _COLORS = cs || ['#e11', '#44f', '#d0d0d0'];\n        let colorsDiv = Lab.ui.createGroup('column');\n        mainDiv.add(colorsDiv);\n        colorsDiv.add(Lab.ui.createBaseText('主题色设置'));\n        colorsDiv.style = `display: flex;\n            justify-content: space-around;\n            margin: 12px;`;\n        let incDivs = Array.from(_COLORS, (c, index) => {\n            let inc = Lab.ui.createInput('color', index, ds => {\n                if (ds[0] && ds[0] != c) _COLORS[index] = ds[0];\n            });\n            // console.log(inc,c)\n            c = new Lab.Color(c).hex();\n            inc.setDefaultValue([c]);\n            colorsDiv.add(inc);\n            return inc\n        });\n\n        let getColorsBtn = Lab.ui.createButton('从图片提取主题色', async() => {\n            let im = Lab.clipboard.read('img');\n            if (!im.isEmpty()) {\n                im = await Lab.ui.createImage(im.toDataURL());\n                let cFn = new Lab.Color();\n                let colorsPalette = await cFn.getPalette(im);\n                colorsPalette.forEach((c, index) => {\n                    c = new Lab.Color(c).hex();\n                    _COLORS[index] = c;\n                    incDivs[index].setDefaultValue([c]);\n                })\n            }\n\n        });\n        getColorsBtn.style = `margin:12px;`;\n        mainDiv.add(getColorsBtn);\n\n    });\n};\n\nfunction createWordcloud(canvas, text = '') {\n    let list = Array.from(text.split('\\n'),\n        tx => tx.trim() ? tx.split(\" \") : null).filter(f => f);\n    // 排序,按照词频\n    list = Array.from(list, t => [t[0].trim(), parseInt(t[1].trim())]).sort((a, b) => b[1] - a[1]);\n    let sumValue = Array.from(list, t => t[1]).reduce((acc, val) => Number(acc) + Number(val), 0);\n    list = Array.from(list, t => [t[0], 100 * (t[1] / sumValue)]);\n\n    let wordsColorIndex = Array.from(list, t => t[0]);\n    Lab.Wordcloud(canvas, {\n        list,\n        ... {\n            // gridSize: Math.round(16 * canvas.width / 1024),\n            // weightFactor: function (size) {\n            //     return Math.pow(size, 2.3) * canvas.width / 1024;\n            // },\n            weightFactor: 3,\n            fontFamily: 'monospace',\n            color: function(word, weight, fontSize, distance, theta) {\n                let i = wordsColorIndex.indexOf(word) / wordsColorIndex.length;\n                let c = _COLORS[Math.floor(_COLORS.length * i)];\n                //console.log(word, wordsColorIndex,_COLORS)\n                // console.log(\"%c%s\", `background:${c}`,\"    \");\n                return c\n            },\n            backgroundColor: 'rgba(0,0,0,0)',\n            rotateRatio: 0,\n            drawOutOfBound: false,\n            shrinkToFit: true\n        }\n    });\n}\n\nfunction createCard(canvas, t) {\n    let startTime = performance.now();\n    Lab.ui.loading(0);\n    createWordcloud(canvas, t);\n    db.set('data', t);\n    setTimeout(() => Lab.ui.loading(100), Math.max(0, 1000 - performance.now() + startTime));\n}",
