  "code": "\n// 智能裁切的开关有bug\n\nvar mainDiv;\n\nvar layout = new Lab.Canvas(300, 300);\nvar strokeWidth = 12, w = h = 300, strokeColor = 'black';\nvar _WIDTH = 600, _HEIGHT = 1200;\nvar mainBoard;\n\nvar isDoing = false;\nvar _ISAUTOCROP = true;\nvar _URLS;\n\nvar store=new Lab.Store('unsplash');\n\nfunction gui() {\n    Lab.ui.p5Show(false);\n    \n    mainDiv=Lab.ui.createGroup('container');\n    mainDiv.style.width='100%';\n    let controlDiv=Lab.ui.createGroup('column');\n    controlDiv.style.display='flex';\n    controlDiv.style.justifyContent='space-around';\n    let controlBtns=Lab.ui.createGroup('column');\n    \n    let range = Lab.ui.createInput('range', '布局', async ns => {\n        n = ~~ns[0];\n        w = ~~(_WIDTH / n) - 12;\n        range.input.disabled = true;\n        range.input.max = 10;\n        range.input.min = 1;\n        range.input.step = 1;\n        // console.log(range.input)\n        await start(_URLS);\n        range.input.disabled = false;\n    });\n\n    let rangeWidth = Lab.ui.createInput('range', '描边', async ns => {\n        strokeWidth = ~~ns[0];\n        rangeWidth.input.disabled = true;\n        rangeWidth.input.max = 100;\n        rangeWidth.input.min = 12;\n        rangeWidth.input.step = 2;\n        // console.log(range.input)\n        await start(_URLS);\n        rangeWidth.input.disabled = false;\n    });\n\n    let colorIn = Lab.ui.createInput('color', '颜色', async ns => {\n        strokeColor = ns[0];\n        await start(_URLS);\n    });\n\n    \n    \n    let inImgs = Lab.ui.createInput('imgs', '多图', start, true, false);\n    let plusBtn = Lab.ui.createIcon('plus', () => {\n        let im = Lab.clipboard.read('img');\n        if (im) {\n            inImgs.add(im.toDataURL());\n        };\n    });\n    let clearBtn = Lab.ui.createIcon('clear', () => {\n        layout.reset();\n        inImgs.reset();\n    });\n    let downloadBtn = Lab.ui.createButton('另存为', () => {\n        Lab.ui.saveBase64Dialog(\n            layout.exportImage(mainBoard, 4, 'png')\n        );\n    },false);\n\n\n    let imgInputs = Lab.ui.createGroup('column');\n    \n    let btns=Lab.ui.createGroup('column');\n\n    let unsplash=Lab.ui.createGroup('card');\n    let token;\n    let inputToken=Lab.ui.createInput('text','Unsplash Access Key',e=>{\n        token=e[0];\n    });\n    let unsplashData=[];\n    let runUnsplash=Lab.ui.createButton('获取',async()=>{\n       let res=await Lab.Image.getUnsplash(token);\n       console.log(res)\n       if(res) {\n           for (const r of res) {\n               if(!unsplashData.includes(r.urls.small)) unsplashData.push(r.urls.small);\n           }\n       };\n       store.set('data', unsplashData);\n       let nds=[...Lab.base.shuffle(unsplashData)].slice(0,5);\n       start(nds);\n    });\n\n\n    mainDiv.add(layout.getElement());\n    mainDiv.add(controlDiv);\n\n    controlDiv.add(controlBtns);\n    controlBtns.add(range);\n    controlBtns.add(rangeWidth);\n    controlBtns.add(colorIn);\n   \n    controlDiv.add(imgInputs);\n    imgInputs.add(inImgs);\n    imgInputs.add(btns);\n    btns.add(plusBtn);\n    btns.add(clearBtn);\n    btns.add(downloadBtn);\n\n    unsplash.add(inputToken);\n    unsplash.add(runUnsplash);\n    mainDiv.add(unsplash);\n     \n    Lab.ui.add(mainDiv);\n    // g.style.width = 'calc(100% - 56px)';\n    // g.style.padding = '12px';\n    \n    // setTimeout(() => Lab.ui.loading(100), 1000);\n\n    store.get('data').then(d=>unsplashData=d);\n};\n\n\nasync function start(data) {\n    if (isDoing) return;\n    if (!data) return;\n    layout.reset();\n    isDoing = true;\n    data = Lab.base.shuffle(data);\n    await doImgs(data);\n    isDoing = false;\n}\n\nasync function doImgs(urls) {\n    _URLS = urls;\n    let imgs = [];\n    // let g=Lab.ui.createGroup()\n    for (let url of urls) {\n        let im = await Lab.ui.createImage(url);\n        // console.log(im)\n        if (im) imgs.push({\n            img: im,\n            // crop: cropCanvas,\n            width: _ISAUTOCROP == false ? im.width : w,\n            height: _ISAUTOCROP == false ? im.height : h,\n            oWidth: im.width,\n            oHeigth: im.height,\n            ratio: im.width / im.height\n        });\n        // g.appendChild(im)\n    };\n    // Lab.ui.add(g);\n    console.log(imgs);\n\n    let group = new Lab.FlexLayout();\n    await group.init();\n    let groupResult = group.masonry(_WIDTH, _HEIGHT, imgs);\n    // console.log(groupResult)\n    mainBoard = layout.addRect(Object.assign(groupResult.mainStyle, {\n        fill: 'white',\n        left: groupResult.mainStyle.left - strokeWidth,\n        top: groupResult.mainStyle.top - strokeWidth,\n        width: groupResult.mainStyle.width + strokeWidth * 3,\n        height: groupResult.mainStyle.height + strokeWidth * 3\n    }), true, true);\n    groupResult.styles.forEach(async (s, i) => {\n\n        if (_ISAUTOCROP) {\n            let cropCanvas = await Lab.Image.smartCrop(imgs[i].img, s.width, s.height);\n            let canvas = await Lab.ui.createImage(cropCanvas.toDataURL())\n\n            layout.addImg(canvas, {\n                strokeWidth: strokeWidth, stroke: strokeColor,\n                left: s.left, top: s.top, width: s.width, height: s.height\n            });\n        } else {\n            layout.addAndResizeImage(imgs[i].img, {\n                strokeWidth: strokeWidth, stroke: strokeColor,\n                left: s.left, top: s.top, width: s.width, height: s.height\n            })\n        }\n\n        let rect = layout.addRect({\n            strokeWidth: 2,\n            stroke: strokeColor,\n            fill: 'transparent',\n            left: s.left + strokeWidth,\n            top: s.top + strokeWidth,\n            width: s.width - strokeWidth,\n            height: s.height - strokeWidth\n        }, true, true);\n        rect.name = 'grid';\n        await Lab.base.sleep(200);\n        rect.moveTo(groupResult.styles.length * 20)\n    });\n\n    // await Lab.base.sleep(200);\n    // layout.getObjects().forEach(o => o.name === 'grid' ? o.moveTo(groupResult.styles.length * 20) : null);\n\n    await Lab.base.sleep(200);\n    layout.zoomToFitCanvas();\n\n}",
