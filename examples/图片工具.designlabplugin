  "code": "var _IMG_URL;\nLab.Image.initPixelit();\nfunction gui() {\n    // maindiv\n    let maindiv = Lab.ui.createGroup();\n\n    let titleStyle = `font-size: 18px;\n    font-weight: 800;`;\n    // \n    let titleForInput = Lab.ui.createBaseText('1 从文字生成图片');\n    titleForInput.style = titleStyle\n    \n    // 文本\n    let titleInput = Lab.ui.createInput('text', '输入');\n    let text2ImgBtn = Lab.ui.createButton('运行', async() => {\n        let text=titleInput.getValue();\n        if(text){\n            let title = Lab.ui.createTextCanvas2(text,{\n                fontSize:24\n            });\n            let base64 = title.toDataURL();\n            Lab.ui.toast('已写入剪切板');\n            Lab.clipboard.write(base64, 'base64');\n        }\n    });\n\n    // 图片\n    let titleImagePro = Lab.ui.createBaseText('2 转base64');\n    titleImagePro.style = titleStyle;\n\n    let [inputDiv,input,size]=createInputImageUI();\n\n    //创建输入\n    let innerImageBtn = Lab.ui.createButton('运行', async() => {\n        let width = ~~size.querySelector('input').value;\n        if (!_IMG_URL) _IMG_URL = input.getValue();\n        await createInnerImage(_IMG_URL, width);\n    });\n    \n    // 图片马赛克\n    let imgPixel = Lab.ui.createBaseText('3 图片马赛克');\n    imgPixel.style = titleStyle;\n\n    let [inputDivForPixel,inputForPixel,sizeForPixel]=createInputImageUI();\n    \n    let pixelModel=Lab.ui.createModel('结果');\n\n    //创建输入\n    let pixelBtn = Lab.ui.createButton('运行', async() => {\n        let width = ~~sizeForPixel.querySelector('input').value;\n        if (!_IMG_URL) _IMG_URL = inputForPixel.getValue();\n        let im = await Lab.Image.createImage(_IMG_URL);\n        let pixCanvas=Lab.ui.createCanvas(im.width,im.height);\n        pixCanvas.style.width='100%';\n        let px=new Lab.Image.Pixelit({\n            to : pixCanvas,\n            from : im,\n            scale : 8,\n            // palette : [[r,g,b]],\n            maxHeight:null,\n            maxWidth:null,\n          });\n        px.draw().pixelate();\n        pixCanvas.show();\n        pixelModel.clear();\n        pixelModel.add(pixCanvas);\n        pixelModel.show();\n        let base64 = pixCanvas.toDataURL();\n        Lab.ui.toast('已写入剪切板');\n        Lab.clipboard.write(base64, 'base64');\n    });\n \n\n    Lab.ui.add(maindiv);\n\n    maindiv.add(Lab.ui.createDivider());\n    // 标题\n    maindiv.add(titleForInput);\n    maindiv.add(titleInput);\n    maindiv.add(Lab.ui.createDivider());\n    maindiv.add(text2ImgBtn);\n    maindiv.add(Lab.ui.createDivider());\n    maindiv.add(Lab.ui.createDivider());\n\n    // 标题\n    maindiv.add(titleImagePro);\n    maindiv.add(inputDiv);\n    maindiv.add(Lab.ui.createDivider());\n    maindiv.add(innerImageBtn);\n    maindiv.add(Lab.ui.createDivider());\n\n    // 标题\n    maindiv.add(imgPixel);\n    maindiv.add(Lab.ui.createDivider());\n    maindiv.add(inputDivForPixel);\n    maindiv.add(pixelModel);\n    maindiv.add(Lab.ui.createDivider());\n    maindiv.add(pixelBtn);\n    maindiv.add(Lab.ui.createDivider());\n\n}\n\n\nfunction createInputImageUI(){\n\n    let inputDiv = Lab.ui.createGroup();\n    inputDiv.style = `display: flex;\n    justify-content: space-around;\n    align-items: center;`;\n\n    let size = Lab.ui.createInput('text', '固定宽度'); \n\n    // 上传控件\n    let input=Lab.ui.createImageAndPreview();\n    let inputModel=Lab.ui.createModel('导入');\n\n    let openBtn=Lab.ui.createButton('从本地',async ()=>{\n       let filePaths= Lab.ui.getFilePath(1,'打开');\n    //    console.log(filePaths)\n       if(filePaths&&filePaths[0]){\n            _IMG_URL=await Lab.Image.getNativeImageFromWebview2(filePaths[0]);\n            input.setValue([_IMG_URL]);\n            let im = await Lab.Image.createImage(_IMG_URL);\n            size.setDefaultValue([im.naturalWidth]);\n       };\n       Lab.ui.loading(100);   \n       inputModel.hide();\n    });\n\n    let pasteBtn = Lab.ui.createButton('从剪切板拷贝图片', () => {\n        let im = Lab.clipboard.read('img');\n        if (im && im.getSize() && (im.getSize()).width > 0) {\n            Lab.ui.loading(0);\n            _IMG_URL = im.toDataURL();\n            input.setValue([_IMG_URL]);\n            size.setDefaultValue([im.getSize().width]);\n        };\n        Lab.ui.loading(100);   \n        inputModel.hide();\n    });\n    let pasteUrlBtn = Lab.ui.createButton('从剪切板URL', async() => {\n        let url = Lab.clipboard.read('text');\n        if (url) {\n            Lab.ui.loading(0);\n            let base64 = await Lab.Image.getNativeImageFromWebview2(url);\n            if(base64){\n                _IMG_URL=base64;\n                let im = await Lab.Image.createImage(_IMG_URL);\n                input.setValue([_IMG_URL]);\n                size.setDefaultValue([im.naturalWidth]);\n            }else{\n                Lab.ui.toast('请复制图片地址');\n            };\n        };\n        Lab.ui.loading(100);   \n        inputModel.hide();\n    });\n\n    inputModel.add(openBtn);\n    inputModel.add(Lab.ui.createDivider());\n    inputModel.add(pasteBtn);\n    inputModel.add(Lab.ui.createDivider());\n    inputModel.add(pasteUrlBtn);\n    \n    input.addEventListener('click', e => {\n        Lab.ui.loading(0);\n        inputModel.show();\n        setTimeout(()=>Lab.ui.loading(100),3000);\n    });\n\n    inputDiv.add(input);\n    inputDiv.add(size);\n    inputDiv.add(inputModel);\n\n    return [inputDiv,input,size]\n}\n\n// \nasync function createInnerImage(base64, width = 300) {\n    // 用于压缩图片\n    Lab.ui.loading(0);\n    let img = await Lab.ui.createImage(base64);\n    // console.log(img)\n    // console.log(img.naturalWidth)\n    if (img.naturalWidth > width) {\n        let height = parseInt(img.naturalHeight * width / img.naturalWidth);\n        let canvas = Lab.ui.createCanvas(100, 100, 'canvas', 'myCanvas', true);\n        let ctx = canvas.getContext('2d');\n        canvas.width = width;\n        canvas.height = height;\n        ctx.drawImage(img, 0, 0, width, height);\n        base64 = canvas.toDataURL();\n    } else {\n        width = img.naturalWidth;\n    }\n    // let html = `<img src='${base64}' style='width:${width}px;height:${height}px'/>`;\n    Lab.ui.toast(`宽度${width}px 已写入剪切板`);\n    Lab.clipboard.write(base64, 'base64');\n    Lab.ui.loading(100);\n\n    let {id,name}=Lab.base.getAppId();\n    let s=new Lab.Store();\n    if(id) s.set(id,{\n        id,name,data:base64\n    })\n}",
