  "code": "var mainDiv, text, emojis, grids;\n\nvar _KEY = 'emoji';\n// 数据\nvar dataset = {};\nvar db = new Lab.Store(_KEY);\nvar _INDEX;\n\nfunction gui() {\n    Lab.ui.p5Show(false);\n\n    mainDiv = Lab.ui.createGroup('container');\n    Lab.ui.add(mainDiv);\n    mainDiv.style = `width: 100%;`;\n\n    let topDiv = Lab.ui.createGroup('column');\n    topDiv.style.display = 'flex';\n    mainDiv.add(topDiv);\n\n    let inputEmoji = Lab.ui.createGroup('column');\n    topDiv.add(inputEmoji);\n\n    createCard(inputEmoji);\n\n    let outputDiv = Lab.ui.createGroup('cards');\n    topDiv.add(outputDiv);\n    outputDiv.style = `display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;`;\n\n    let gridStyle = `width:88px;height:88px;border: 1px solid #eee;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    `;\n\n    grids = [\n        Lab.ui.createGroup('column'),\n        Lab.ui.createGroup('column'),\n        Lab.ui.createGroup('column')\n    ];\n    Array.from(grids, (g, id) => {\n        g.style = gridStyle;\n        g.classList.add('ui');\n        // g.classList.add('red');\n        g.classList.add('card');\n        outputDiv.add(g);\n        g.addEventListener('click', e => {\n            Array.from(grids, (ng, nid) => nid != id ? ng.classList.remove('red') : null);\n            g.classList.toggle('red');\n\n            if (g.classList.contains('red')) {\n                _INDEX = id;\n            } else {\n                _INDEX = null;\n            }\n\n            emojis.clear();\n            if (Array.from(grids, ng => ng.classList.contains('red')).filter(f => f).length > 0) {\n                emojis.classList.add('red');\n                console.log(_INDEX);\n                if (_INDEX != null && dataset[_INDEX]) {\n                    let es = dataset[_INDEX];\n                    Array.from(es, emo => {\n                        let emojiDiv = createEmoji(emo);\n                        emojis.add(emojiDiv);\n                    });\n                }\n\n            } else {\n                emojis.classList.remove('red');\n            }\n\n        });\n    });\n\n\n    emojis = Lab.ui.createGroup('column');\n    emojis.className = 'ui column card';\n    mainDiv.add(emojis);\n    emojis.style = `display: flex;\n    flex-direction: row;\n    margin: 12px;\n    flex-wrap: wrap;\n    min-height:56px;\n    width: calc(100% - 24px);`;\n\n\n    let btn = Lab.ui.createButton('合成', createResult);\n    mainDiv.add(btn);\n\n    let resetBtn = Lab.ui.createButton('清空', async() => {\n\n        await db.clear();\n        dataset = {};\n        emojis.clear();\n        resetGrids();\n\n    });\n    mainDiv.add(resetBtn);\n\n    db.get(_KEY).then(res => {\n        if (res) dataset = res;\n        if (dataset && dataset[0] && dataset[1] && dataset[2]) {\n            let count = `数量 ${dataset[0].length+dataset[1].length+dataset[2].length}`;\n            emojis.clear();\n            emojis.add(Lab.ui.createBaseText(count));\n        }\n    });\n\n};\n\nfunction createCard(inputEmoji) {\n    let card = Lab.ui.createGroup('card');\n    inputEmoji.add(card);\n\n    let emoji = Lab.ui.createEmojiPicker();\n    card.add(emoji);\n    emoji.init((event) => {\n        // console.log('!', event.detail.unicode);\n        let emojiDiv = createEmoji(event.detail.unicode);\n        emojis.add(emojiDiv);\n        if (_INDEX != null) {\n            if (!dataset[_INDEX]) dataset[_INDEX] = [];\n            dataset[_INDEX].push(event.detail.unicode);\n            db.set(_KEY, dataset);\n        }\n\n    });\n};\n\nfunction createEmoji(unicode) {\n    let emojiDiv = Lab.ui.createGroup('column');\n    let c = Lab.ui.createTextCanvas(unicode);\n    emojiDiv.add(c);\n    c.style = `background: white;\n        margin: 8px;`;\n    return emojiDiv\n}\n\nfunction createResult() {\n    emojis.classList.remove('red');\n    if (dataset[0] && dataset[1] && dataset[2]) {\n\n        let ds = Lab.base.shuffle(Lab.base.cartesian([dataset[0], dataset[1], dataset[2]]))[0];\n        if (grids) {\n            Lab.clipboard.write(ds.join(' '), 'text');\n            Array.from(grids, (g, i) => {\n                g.clear();\n                g.classList.remove('red');\n                let e = createEmoji(ds[i]);\n                g.add(e);\n            });\n        }\n        return ds\n    };\n}\n\nfunction resetGrids() {\n    Array.from(grids, (g, i) => {\n        g.clear();\n        g.classList.remove('red');\n    });\n}",
