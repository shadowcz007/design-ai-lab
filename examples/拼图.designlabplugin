  "code": "\n// 智能裁切的开关有bug\n\nvar layout = new Canvas(400, 400);\nvar strokeWidth = 12, w = h = 300, strokeColor = 'black';\nvar _WIDTH = 600, _HEIGHT = 1200;\nvar mainBoard;\n\nvar isDoing = false;\nvar _ISAUTOCROP = true;\nvar _URLS;\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    Lab.base.loading(0);\n    let range = Lab.base.createInput('range', '布局', async ns => {\n        n = ~~ns[0];\n        w = ~~(_WIDTH / n) - 12;\n        range.input.disabled = true;\n        range.input.max = 10;\n        range.input.min = 1;\n        range.input.step = 1;\n        // console.log(range.input)\n        await start(_URLS);\n        range.input.disabled = false;\n    });\n\n    let rangeWidth = Lab.base.createInput('range', '描边', async ns => {\n        strokeWidth = ~~ns[0];\n        rangeWidth.input.disabled = true;\n        rangeWidth.input.max = 100;\n        rangeWidth.input.min = 12;\n        rangeWidth.input.step = 2;\n        // console.log(range.input)\n        await start(_URLS);\n        rangeWidth.input.disabled = false;\n    });\n\n    let colorIn = Lab.base.createInput('color', '颜色', async ns => {\n        strokeColor = ns[0];\n        await start(_URLS);\n    });\n\n    // let check = Lab.base.createInput('check', '智能裁切', async ns => {\n    //     check.input.disabled = true;\n    //     _ISAUTOCROP = ns[0];\n    //     console.log(ns[0])\n    //     await start(_URLS);\n    //     check.input.disabled = false;\n    // });\n    let download = Lab.base.createButton('另存为', () => {\n        Lab.base.saveBase64Dialog(\n            layout.exportImage(mainBoard, 4, 'png')\n        );\n    });\n\n\n    let inImgs = Lab.base.createInput('imgs', '多图', start, true, false);\n\n    let g = Lab.base.createGroup(inImgs, download);\n    g.layout(1, false);\n    g.style.width = 'calc(100% - 56px)';\n    g.style.padding = '12px';\n    Lab.base.add(g);\n\n    Lab.base.add(layout.getElement());\n\n    setTimeout(() => Lab.base.loading(100), 1000);\n};\n\n\nasync function start(data) {\n    if (isDoing) return;\n    if (!data) return;\n    layout.reset();\n    isDoing = true;\n    data = Lab.base.shuffle(data);\n    await doImgs(data);\n    isDoing = false;\n}\n\nasync function doImgs(urls) {\n    _URLS = urls;\n    let imgs = [];\n    // let g=Lab.base.createGroup()\n    for (let url of urls) {\n        let im = await Lab.base.createImage(url);\n        imgs.push({\n            img: im,\n            // crop: cropCanvas,\n            width: _ISAUTOCROP == false ? im.width : w,\n            height: _ISAUTOCROP == false ? im.height : h,\n            oWidth: im.width,\n            oHeigth: im.height,\n            ratio: im.width / im.height\n        });\n        // g.appendChild(im)\n    };\n    // Lab.base.add(g);\n    console.log(imgs);\n\n    let group = new Lab.FlexLayout();\n    await group.init();\n    let groupResult = group.masonry(_WIDTH, _HEIGHT, imgs);\n    // console.log(groupResult)\n    mainBoard = layout.addRect(Object.assign(groupResult.mainStyle, {\n        fill: 'white',\n        left: groupResult.mainStyle.left - strokeWidth,\n        top: groupResult.mainStyle.top - strokeWidth,\n        width: groupResult.mainStyle.width + strokeWidth * 3,\n        height: groupResult.mainStyle.height + strokeWidth * 3\n    }), true, true);\n    groupResult.styles.forEach(async (s, i) => {\n\n        if (_ISAUTOCROP) {\n            let cropCanvas = await Lab.ai.smartCrop(imgs[i].img, s.width, s.height);\n            let canvas = await Lab.base.createImage(cropCanvas.toDataURL())\n\n            layout.addImg(canvas, {\n                strokeWidth: strokeWidth, stroke: strokeColor,\n                left: s.left, top: s.top, width: s.width, height: s.height\n            });\n        } else {\n            layout.addAndResizeImage(imgs[i].img, {\n                strokeWidth: strokeWidth, stroke: strokeColor,\n                left: s.left, top: s.top, width: s.width, height: s.height\n            })\n        }\n\n        let rect = layout.addRect({\n            strokeWidth: 2,\n            stroke: strokeColor,\n            fill: 'transparent',\n            left: s.left + strokeWidth,\n            top: s.top + strokeWidth,\n            width: s.width - strokeWidth,\n            height: s.height - strokeWidth\n        }, true, true);\n        rect.name = 'grid';\n        await Lab.base.sleep(200);\n        rect.moveTo(groupResult.styles.length * 20)\n    });\n\n    // await Lab.base.sleep(200);\n    // layout.getObjects().forEach(o => o.name === 'grid' ? o.moveTo(groupResult.styles.length * 20) : null);\n\n    await Lab.base.sleep(200);\n    layout.zoomToFitCanvas();\n\n}",
