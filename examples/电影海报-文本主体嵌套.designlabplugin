true, false);\n  // Lab.ui.add(backgroundImgIn);\n\n  let plusIcon = Lab.ui.createIcon('plus', () => {\n    let im = Lab.clipboard.read('img');\n    if (im) doImage([im.toDataURL()]);\n  }, false),\n    copyIcon = Lab.ui.createIcon('copy', () => {\n      Lab.base.clipboard.write(layout.exportImage(bgImg, 4, 'png'), 'base64')\n    }, false),\n    downloadIcon = Lab.ui.createIcon('download', () => {\n      Lab.ui.saveBase64Dialog(layout.exportImage(bgImg, 4, 'png'))\n    }, false);\n  let btns2 = Lab.ui.createGroup(backgroundImgIn,\n    colorIn,\n    plusIcon,\n    copyIcon,\n    downloadIcon);\n  // Lab.ui.add(btns2);\n\n  let g1 = Lab.ui.createGroup(btns1, btns2);\n  let g2 = Lab.ui.createGroup(layout.getElement());\n  let g3 = Lab.ui.createGroup(g1, g2);\n  Lab.ui.add(g3);\n  g3.layout(7);\n  g3.style.flexWrap = 'nowrap';\n};\n\nfunction randomGetone(ts) {\n  let i = Math.ceil(\n    Math.random() * ts.length) - 1;\n  return ts[i]\n};\n\nfunction resizeRect(rect, seg) {\n  if (rect.width < seg.width * 0.6) {\n    rect.width = seg.width * 0.6;\n    rect.x = rect.x - (seg.width * 0.6 - rect.width) / 2;\n    if (rect.x < 0) rect.x = 0;\n  };\n  if (rect.x + rect.width > seg.width) {\n    rect.width = seg.width - rect.x;\n  };\n  if (rect.y > seg.height * 0.7) {\n    rect.y = seg.height * 0.3;\n  };\n  console.log(rect, seg.width, seg.height)\n  return rect\n}\n\nasync function updateLogo(url) {\n  if (!(url && url[0])) return\n  url = url[0];\n  logo = await Lab.ui.createImage(url);\n}\n\nvar isDoing = false;\nasync function doImage(urls) {\n  if (isDoing) return;\n  isDoing = true;\n  if (!(urls && urls[0])) return;\n  originBgImg = urls;\n  let url = urls[0];\n  layout.reset();\n  let im = await Lab.ui.createImage(url);\n  bgImg = layout.addImg(im, {}, true, true);\n  layout.gray(bgImg);\n  layout.blur(bgImg);\n  await doResearch(im);\n  layout.zoomToFitCanvas();\n  isDoing = false;\n};\n\nasync function doResearch(img) {\n  // 抠图\n  let seg;\n  if (modelType === 0) {\n    seg = await u2net.segment(img);\n  } else if (modelType === 1) {\n    seg = await Lab.ai.humanseg.start(img);\n  } else if (modelType === 2) {\n    seg = await Lab.ai.bodypix.segmentPerson(img);\n  }\n\n  // console.log('seg', seg)\n  // 主色\n  if (!isFixed) color = await Lab.Color.getColor(seg) || [255, 255, 255];\n  // console.log(color)\n\n\n  // 外围父级轮廓\n  let rect = Lab.shape.getBoundingRect(seg);\n  //console.log(rect)\n  if (rect) {\n    // 判断构图方式\n    layoutClassify(rect, seg.width, seg.height);\n    // 调整主体范围\n    rect = resizeRect(rect, seg);\n    console.log(color)\n    var t = {\n      fill: `rgba(${color.join(',')},0.8)`,\n      //stroke: 'rgba(0,0,255,1)',\n      // width: rect.width,\n      // height: rect.height,\n      width: img.width,\n      // left: rect.x,\n      // top: rect.y,\n      left: 0,\n      top: 0,\n      // width: img.width / 2,\n      height: img.height\n    }\n    addRect(t);\n  };\n\n  let scale = 0.8 * seg.width / logo.width;\n  let logoTop = seg.height * 0.2;\n\n  layout.addImg(logo, {\n    lockMovementX: false,\n    lockRotation: false,\n    lockScalingX: false,\n    lockScalingY: false,\n    left: 0.1 * seg.width,\n    top: logoTop,\n    scaleX: scale,\n    scaleY: scale\n  }, true, true);\n\n  layout.addImg(seg, {\n    lockMovementX: false,\n    lockRotation: false,\n    lockScalingX: false,\n    lockScalingY: false,\n  }, true, true);\n\n  if (!isFixed) mainText = randomGetone(texts);\n  layout.addText(mainText, {\n    fontSize: logo.height * scale * 0.3,\n    fontFamily: 'monospace',\n    fill: \"black\",\n    //stroke: 'rgba(0,0,0,0.8)',\n    left: seg.width * 0.1,\n    top: logoTop + logo.height * scale,\n    width: seg.width * 0.4,\n    height: seg.height * 0.25\n  }, true, true);\n\n  layout.addText(author, {\n    fontSize: logo.height * scale * 0.2,\n    fontFamily: 'monospace',\n    fill: \"black\",\n    //stroke: 'rgba(0,0,0,0.8)',\n    textAlign: 'right',\n    left: seg.width / 2,\n    top: seg.height - logo.height * scale * 0.2 - 24,\n    width: seg.width / 2 - 24\n  }, true, true);\n};\n\n\n\n\nfunction layoutClassify(rect, w, h) {\n  console.log(rect, w, h)\n  let c = document.createElement('canvas');\n  c.width = w;\n  c.height = h;\n\n  let ctx = c.getContext('2d');\n  ctx.fillStyle = 'black';\n  ctx.fillRect(rect.x, rect.y, rect.width, rect.height);\n\n  let nc = document.createElement('canvas');\n  nc.width = 9;\n  nc.height = 9;\n  var nctx = nc.getContext('2d');\n  nctx.drawImage(c, 0, 0, c.width, c.height, 0, 0, 9, 9);\n\n  console.log(nctx)\n}\n\nfunction addRect(style) {\n  layout.addRect(Object.assign(style, {\n    lockMovementX: false,\n    lockRotation: false,\n    lockScalingX: false,\n    lockScalingY: false,\n  }), true, true)\n}",
