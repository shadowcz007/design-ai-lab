  "code": "//Hello AI world!\nvar layout = new Canvas(300, 300, false, true);\n// 双击移动到最顶部\nlayout.dblclick = opt => {\n    layout.moveToTop(opt.target);\n};\n\n\nvar u2net = Lab.ai.u2net;\nvar modelType = 0;\n\nvar logo, mainText, color, isFixed = false, bgImg;\n\nvar texts = [\n    '世界上一切可以被称为艺术的东西 都需要天赋',\n    '你们是唯一变量',\n    '只要我一直找下去 总有一天 我能找到',\n    '只要我一直写下去 我活着 就有意义了',\n    '相信就能实现', \n    '凡是有意义的事都不会容易',\n    '我要像你一样就好了'\n];\n\nvar author='by design-ai-lab';\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    Lab.base.createInput('img', 'background', doImage);\n    Lab.base.createInput('img', 'logo', updateLogo);\n    let icon = Lab.base.createIcon('thumbtack', e => {\n        this.isFixed = !this.isFixed;\n        icon.style.background = this.isFixed && color ? `rgba(${color.join(',')},0.8)` : '';\n    });\n\n    let inSelect = Lab.base.createInput('select', '选择', data => {\n        modelType = parseInt(data);\n    }, true, false);\n    inSelect.appendOptions([{\n        text: 'u2net',\n        value: 0\n    }, {\n        text: 'humanseg',\n        value: 1\n    }, {\n        text: 'bodypix',\n        value: 2\n    }]);\n    let btns1 = Lab.base.createGroup(inSelect);\n    Lab.base.add(btns1);\n\n    let plus = Lab.base.createIcon('plus', () => {\n        let im = Lab.base.clipboard.read('img');\n        if (im) doImage([im.toDataURL()]);\n    }, false),\n        copy = Lab.base.createIcon('copy', () => {\n            Lab.base.clipboard.write(layout.exportImage(bgImg, 4, 'png'), 'base64')\n        }, false),\n        download = Lab.base.createIcon('download', () => {\n            Lab.base.saveBase64Dialog(layout.exportImage(bgImg, 4, 'png'))\n        }, false);\n    let btns2 = Lab.base.createGroup(plus, copy, download);\n    Lab.base.add(btns2);\n\n    Lab.base.add(layout.getElement());\n};\n\nfunction randomGetone(ts) {\n    let i = Math.ceil(\n        Math.random() * ts.length) - 1;\n    return ts[i]\n};\n\nfunction resizeRect(rect, seg) {\n    if (rect.width < seg.width * 0.6) {\n        rect.width = seg.width * 0.6;\n        rect.x = rect.x - (seg.width * 0.6 - rect.width) / 2;\n        if (rect.x < 0) rect.x = 0;\n    };\n    if (rect.x + rect.width > seg.width) {\n        rect.width = seg.width - rect.x;\n    };\n    if (rect.y > seg.height * 0.7) {\n        rect.y = seg.height * 0.3;\n    };\n    console.log(rect, seg.width, seg.height)\n    return rect\n}\n\nasync function updateLogo(url) {\n    if (!(url && url[0])) return\n    url = url[0];\n    logo = await Lab.base.createImage(url);\n}\n\nasync function doImage(url) {\n    if (!(url && url[0])) return\n    url = url[0];\n    layout.reset();\n    let im = await Lab.base.createImage(url);\n    bgImg = layout.addImg(im, {\n        lockMovementX: true,\n        lockRotation: true,\n        lockScalingX: true,\n        lockScalingY: true,\n    }, false, false);\n    layout.gray(bgImg);\n    layout.blur(bgImg);\n    await doResearch(im);\n\n    layout.zoomToFitCanvas();\n};\n\nasync function doResearch(img) {\n\n    // 抠图\n    let seg;\n    if (modelType === 0) {\n        seg = await u2net.segment(img);\n    } else if (modelType === 1) {\n        seg = await Lab.ai.humanseg.start(img);\n    } else if (modelType === 2) {\n        seg = await Lab.ai.bodypix.segmentPerson(img);\n    }\n\n    // console.log(img,seg)\n    // 主色\n    if (!isFixed) color = await Lab.ai.getColor(seg) || [255, 255, 255];\n    // console.log(color)\n\n    let rect;\n    // 外围父级轮廓\n    let rects = Lab.ai.shape.getBoundingRect(seg);\n    if (rects.length > 0) {\n        // console.log(rects)\n        rect = rects[0];\n        // 判断构图方式\n        layoutClassify(rects[0], seg.width, seg.height);\n        // 调整主体范围\n        rect = resizeRect(rect, seg);\n        var t = {\n            fill: `rgba(${color.join(',')},0.8)`,\n            //stroke: 'rgba(0,0,255,1)',\n            // width: rect.width,\n            // height: rect.height,\n            width: img.width,\n            // left: rect.x,\n            // top: rect.y,\n            left: 0,\n            top: 0,\n            // width: img.width / 2,\n            height: img.height\n        }\n        addRect(t);\n    };\n\n    let scale = 0.8 * seg.width / logo.width;\n    let logoTop = seg.height * 0.2;\n\n    layout.addImg(logo, {\n        lockMovementX: false,\n        lockRotation: false,\n        lockScalingX: false,\n        lockScalingY: false,\n        left: 0.1 * seg.width,\n        top: logoTop,\n        scaleX: scale,\n        scaleY: scale\n    }, true, true);\n\n    layout.addImg(seg, {\n        lockMovementX: false,\n        lockRotation: false,\n        lockScalingX: false,\n        lockScalingY: false,\n    }, true, true);\n\n    if (!isFixed) mainText = randomGetone(texts);\n    layout.addText(mainText, {\n        fontSize: logo.height * scale * 0.3,\n        fontFamily: 'monospace',\n        fill: \"black\",\n        //stroke: 'rgba(0,0,0,0.8)',\n        left: seg.width * 0.1,\n        top: logoTop + logo.height * scale,\n        width: seg.width * 0.4\n    });\n\n    layout.addText(author, {\n        fontSize: logo.height * scale * 0.2,\n        fontFamily: 'monospace',\n        fill: \"black\",\n        //stroke: 'rgba(0,0,0,0.8)',\n        textAlign: 'right',\n        left: seg.width / 2,\n        top: seg.height - logo.height * scale * 0.2 - 24,\n        width: seg.width / 2 - 24\n    });\n};\n\n\n\n\nfunction layoutClassify(rect, w, h) {\n    console.log(rect, w, h)\n    let c = document.createElement('canvas');\n    c.width = w;\n    c.height = h;\n\n    let ctx = c.getContext('2d');\n    ctx.fillStyle = 'black';\n    ctx.fillRect(rect.x, rect.y, rect.width, rect.height);\n\n    let nc = document.createElement('canvas');\n    nc.width = 9;\n    nc.height = 9;\n    var nctx = nc.getContext('2d');\n    nctx.drawImage(c, 0, 0, c.width, c.height, 0, 0, 9, 9);\n\n    console.log(nctx)\n}\n\nfunction addRect(style) {\n    layout.addRect(Object.assign(style, {\n        lockMovementX: false,\n        lockRotation: false,\n        lockScalingX: false,\n        lockScalingY: false,\n    }), true, true)\n}",
