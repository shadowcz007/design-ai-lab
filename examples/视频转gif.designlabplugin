  "code": "\nvar width = 220, height = 200, fps = 10, startTime = 0, loop = 5;\n\nvar _FILE;\n\nfunction gui() {\n    Lab.base.p5Show(false);\n    let p = Lab.base.createBaseText('---');\n    p.style.minHeight = '120px';\n    p.style.minWidth = '120px';\n\n    let openBtn = Lab.base.createButton(\"打开\", async () => {\n        let v = await Lab.base.createShortVideoInput();\n        if (v[0].type === 'video' || v[0].type === 'gif') {\n            p.innerText = '正在ing';\n            _FILE = v[0];\n            let info = await Lab.video.getMediaDurationAndType(_FILE.url);\n            p.innerText = JSON.stringify(info, null, 2);\n            _FILE = Object.assign(_FILE, info);\n            width = _FILE.width;\n            height = _FILE.height;\n            fps = _FILE.frame_rate || 0;\n            loop = _FILE.duration;\n            iw.input.value = width;\n            ih.input.value = height;\n            ifps.input.value = fps;\n            iloop.input.value = loop;\n            istartTime.input.value = 0;\n        }\n\n    }, false);\n\n    let btn0 = Lab.base.createButton(\"提取图片\", async () => {\n        // let v = await Lab.base.createShortVideoInput();\n        if (_FILE.type === 'video' || _FILE.type === 'gif') {\n            p.innerText = '正在ing';\n            // let i\n            // \n            // width=\n            let info = await Lab.video.getMediaDurationAndType(_FILE.url);\n            let output = await Lab.video.videoExtract(_FILE.url, info.width + 'x' + info.height, null, 0, 0);\n            info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n    let btn = Lab.base.createButton(\"视频转动图\", async () => {\n        // let v = await Lab.base.createShortVideoInput();\n        if (_FILE.type === 'video' || _FILE.type === 'gif') {\n            p.innerText = '正在ing';\n            // let i\n            // \n            // width=\n            let output = await Lab.video.video2gif(_FILE.url, width, fps, startTime, loop);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n    let btn2 = Lab.base.createButton(\"视频转mp4\", async () => {\n        // let v = await Lab.base.createShortVideoInput();\n        if (_FILE.type === 'video' || _FILE.type === 'gif') {\n            p.innerText = '正在ing';\n            let output = await Lab.video.video2mp4(_FILE.url);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n\n    let btn3 = Lab.base.createButton(\"动图转视频\", async () => {\n        // let v = await Lab.base.createShortVideoInput();\n        // console.log(v)\n        if (_FILE.type === 'gif') {\n            p.innerText = '正在ing';\n            let output = await Lab.video.gif2video(_FILE.url);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n\n    let btn4 = Lab.base.createButton(\"多图转动图\", async () => {\n        let v = await Lab.base.getFilePath();\n        // console.log(v)\n        if (v&&v[0]) {\n            p.innerText = '正在ing';\n            let output = await Lab.video.frames2video(v[0], width + 'x' + height, fps);\n            output = await Lab.video.video2gif(output, width, fps, startTime, loop);\n            let info = await Lab.video.getMediaDurationAndType(output);\n            p.innerText = JSON.stringify(info, null, 2);\n        }\n\n    }, false);\n    // let p=Lab.base.createBaseText('---');\n\n    let g = Lab.base.createGroup(p, openBtn, btn0, btn, btn2, btn3, btn4);\n    g.layout(1);\n    // Lab.base.add(g);\n\n    let iw = Lab.base.createInput('text', '宽度', text => {\n        if (text && text[0]) width = parseInt(text[0]);\n    }, false),\n        ih = Lab.base.createInput('text', '高度', text => {\n            if (text && text[0]) height = parseInt(text[0]);\n        }, false),\n        ifps = Lab.base.createInput('text', '帧速率', text => {\n            if (text && text[0]) fps = parseInt(text[0]);\n        }, false),\n        istartTime = Lab.base.createInput('text', '开始时间', text => {\n            if (text && text[0]) startTime = parseInt(text[0]);\n        }, false),\n        iloop = Lab.base.createInput('text', '终止时间', text => {\n            if (text && text[0]) loop = parseInt(text[0]);\n        }, false);\n\n    let gn = Lab.base.createGroup(iw, ih, ifps, istartTime, iloop);\n    gn.style.minWidth = '200px';\n    gn.style.padding = '12px';\n\n    let gAll = Lab.base.createGroup(g, gn);\n    gAll.layout(5);\n    Lab.base.add(gAll);\n\n}",
